<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.7.0">

    <bannerRight>
        <src>https://www.fossgalaxy.com/branding/logo.png</src>
        <alt>Group Logo</alt>
        <href>https://www.fossgalaxy.com</href>
        <width>120px</width>
        <title>A UnityCoders/FossGalaxy Project</title>
    </bannerRight>

    <!-- use bootstrap theme -->
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>

    <body>
        <links>
            <item name="Home" href="https://www.fossgalaxy.com"/>
            <item name="Forums" href="https://community.fossgalaxy.com"/>
        </links>
        <menu ref="reports"/>
        <menu ref="modules"/>
    </body>

    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>unitycoders/uc_pircbotx</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
</project>